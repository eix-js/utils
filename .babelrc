{
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": false,
				"targets": {
					"browsers": "> 1%, not dead",
					"node": 10
				}
			}
		]
	],
	"plugins": [
		"@babel/plugin-transform-typescript",
		"@babel/plugin-syntax-bigint",
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread"
	]
}
